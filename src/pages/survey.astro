---
import Layout from "@/layouts/Layout.astro";
import SurveyForm from "@/components/SurveyForm.tsx";
import { getCollection } from "astro:content";

const user = Astro.locals.user;

if (!user) {
  return Astro.redirect("/");
}
const questions = await getCollection("axisQuestions");
---

<Layout title="Survey - The Veteran Culture Project" isLandingPage={false}>
  <SurveyForm questions={questions} client:load />
</Layout>
